{
  "appId": "com.example.app",
  "appName": "capgo-test",
  "webDir": "dist",
  "plugins": {
    "CapacitorUpdater": {
      "autoUpdate": false,
      "updateUrl": "https://github.com/GaudRavi93/capgo-test/releases/latest/download/build.zip",
      "channel": "production",
      "timeout": 30000
    },
    "SplashScreen": {
      "launchAutoHide": false,
      "showSpinner": true,
      "spinnerColor": "#ffffff"
    }
  },
  "server": {
    "androidScheme": "https"
  }
}
